<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.8.0" version="2.0" xmlns="http://pear.php.net/dtd/package-2.0" xmlns:tasks="http://pear.php.net/dtd/tasks-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
    http://pear.php.net/dtd/tasks-1.0.xsd
    http://pear.php.net/dtd/package-2.0
    http://pear.php.net/dtd/package-2.0.xsd">
 <name>Etsy_PHPUnit_Extensions</name>
 <channel>etsy.github.com/phpunit-extensions</channel>
 <summary>Etsy extensions for PHPUnit.</summary>
 <description>
   Etsy extensions for PHPUnit.
 </description>
 <lead>
  <name>Laura Beth Lincoln Denker</name>
  <user>elblinkin</user>
  <email>github@elblinkin.info</email>
  <active>yes</active>
 </lead>
 <date>2011-02-27</date>
 <version>
  <release>0.1.0</release>
  <api>0.1.0</api>
 </version>
 <stability>
  <release>beta</release>
  <api>beta</api>
 </stability>
 <license uri="http://www.opensource.org/licenses/mit-license.php">MIT</license>
 <notes>-</notes>
 <contents>
   <dir name="/">
    <file name="LICENSE" role="doc" />
    <file name="README.md" role="doc" />
    <dir name="PHPUnit">
     <dir name="Extensions">
      <dir name="Assert">
       <file name="More.php" role="php" />
      </dir>
      <dir name="Constraint">
       <file name="HasItems.php" role="php" />
       <file name="SameSize.php" role="php" />
       <file name="StringMatchIgnoreWhitespace.php" role="php" />
      </dir>
      <dir name="MultipleDatabase">
       <dir name="DatabaseConfig">
        <file name="Builder.php" role="php" />
       </dir>
       <file name="DatabaseConfig.php" role="php" />
       <file name="TestCase.php" role="php" />
       <file name="Tester.php" role="php" />
      </dir>
      <dir name="TicketListener">
       <dir name="Jira">
        <file baseinstalldir="/" name="Client.php" role="php" />
        <file baseinstalldir="/" name="StatusConverter.php" role="php" />
       </dir>
       <file baseinstalldir="/" name="Jira.php" role="php" />
      </dir>
     </dir>
    </dir>
   </dir>
 </contents>
 <dependencies>
  <required>
   <php>
    <min>5.2.9</min>
   </php>
   <pearinstaller>
    <min>1.9.1</min>
   </pearinstaller>
   <package>
    <name>PHPUnit</name>
    <channel>pear.phpunit.de</channel>
    <min>3.5.12</min>
   </package>
  </required>
 </dependencies>
 <phprelease />
</package>
